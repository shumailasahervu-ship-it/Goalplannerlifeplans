{
  "expo": {
    "name": "life-plan",
    "slug": "life-plan",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "lifeplan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.faran.lifeplans",
      "icon": "./assets/images/ios-light.png",
